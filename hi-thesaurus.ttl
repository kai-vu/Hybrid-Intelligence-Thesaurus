############################################################
# Use Cases Terms
############################################################

hint:UseCase a skos:Concept ;
    skos:inScheme hint:HIScheme ;
    skos:prefLabel "Use Case"@en ;
    skos:definition "A specific application or scenario where Hybrid Intelligence systems are employed to achieve particular goals."@en ;
    skos:exactMatch hi:UseCase .

hint:PersonalAssistantUseCase a skos:Concept ;
    skos:inScheme hint:HIScheme ;
    skos:prefLabel "Personal Assistant Use Case"@en ;
    skos:definition "A use case where Hybrid Intelligence systems function as personal assistants to support users in various tasks."@en ;
    skos:broader hint:UseCase .

