    skos:broader hint:UseCase .

hint:EnergyNegotiationUseCase a skos:Concept ;
    skos:inScheme hint:HIScheme ;
    skos:prefLabel "Energy Negotiation Use Case"@en ;
    skos:definition "A use case where Hybrid Intelligence systems engage in energy negotiation and management tasks."@en ;
    skos:broader hint:UseCase .

hint:VirtualMuseumGuideUseCase a skos:Concept ;
    skos:inScheme hint:HIScheme ;
    skos:prefLabel "Virtual Museum Guide Use Case"@en ;
    skos:definition "A use case where Hybrid Intelligence systems serve as virtual guides in museum settings."@en ;
    skos:broader hint:UseCase .


