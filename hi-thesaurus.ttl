    skos:broader hint:UseCase .

hint:AutonomousTutoringUseCase a skos:Concept ;
    skos:inScheme hint:HIScheme ;
    skos:prefLabel "Autonomous Tutoring Use Case"@en ;
    skos:definition "A use case where Hybrid Intelligence systems provide autonomous tutoring and educational support to learners."@en ;
    skos:broader hint:UseCase .

hint:MedicalDiagnosisUseCase a skos:Concept ;
    skos:inScheme hint:HIScheme ;
    skos:prefLabel "Medical Diagnosis Use Case"@en ;
